{
    "name": "liip/vie-bundle",
    "type": "symfony-bundle",
    "description": "VIE is a way to make any site using RDFa editable.",
    "license": "MIT",
    "authors": [
        {
            "name": "Liip AG",
            "homepage": "http://www.liip.ch/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/liip/LiipVieBundle/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "2.*",
        "symfony/assetic-bundle": ">= 2.0",
        "friendsofsymfony/rest-bundle":"*",
        "midgard/createphp": "dev-master"
    },
    "suggest": {
        "dms/dms-filter": "if you want to filter content on your fields"
    },
    "autoload": {
        "psr-0": { "Liip\\VieBundle": "" }
    },
    "target-dir": "Liip/VieBundle"
}
